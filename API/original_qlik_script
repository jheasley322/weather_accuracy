SET ThousandSep=',';
SET DecimalSep='.';
SET MoneyThousandSep=',';
SET MoneyDecimalSep='.';
SET MoneyFormat='$#,##0.00;-$#,##0.00';
SET TimeFormat='h:mm:ss TT';
SET DateFormat='M/D/YYYY';
SET TimestampFormat='M/D/YYYY h:mm:ss[.fff] TT';
SET FirstWeekDay=6;
SET BrokenWeeks=1;
SET ReferenceDay=0;
SET FirstMonthOfYear=1;
SET CollationLocale='en-US';
SET CreateSearchIndexOnReload=1;
SET MonthNames='Jan;Feb;Mar;Apr;May;Jun;Jul;Aug;Sep;Oct;Nov;Dec';
SET LongMonthNames='January;February;March;April;May;June;July;August;September;October;November;December';
SET DayNames='Mon;Tue;Wed;Thu;Fri;Sat;Sun';
SET LongDayNames='Monday;Tuesday;Wednesday;Thursday;Friday;Saturday;Sunday';
SET NumericalAbbreviation='3:k;6:M;9:G;12:T;15:P;18:E;21:Z;24:Y;-3:m;-6:Î¼;-9:n;-12:p;-15:f;-18:a;-21:z;-24:y';

LIB CONNECT TO 'weather.com_forecast (auth0_joeeasley@odysseylogistics.com)';

RestConnectorMasterTable:
SQL SELECT 
	"__KEY_root",
	(SELECT 
		"language",
		"transaction_id",
		"version",
		"location_id",
		"units",
		"expire_time_gmt",
		"status_code",
		"__FK_metadata"
	FROM "metadata" FK "__FK_metadata"),
	(SELECT 
		"class",
		"expire_time_gmt" AS "expire_time_gmt_u0",
		"fcst_valid",
		"fcst_valid_local",
		"num",
		"day_ind",
		"temp",
		"dewpt",
		"hi",
		"wc",
		"feels_like",
		"icon_extd",
		"wxman",
		"icon_code",
		"dow",
		"phrase_12char",
		"phrase_22char",
		"phrase_32char",
		"subphrase_pt1",
		"subphrase_pt2",
		"subphrase_pt3",
		"pop",
		"precip_type",
		"qpf",
		"snow_qpf",
		"rh",
		"wspd",
		"wdir",
		"wdir_cardinal",
		"gust",
		"clds",
		"vis",
		"mslp",
		"uv_index_raw",
		"uv_index",
		"uv_warning",
		"uv_desc",
		"golf_index",
		"golf_category",
		"severity",
		"__FK_forecasts"
	FROM "forecasts" FK "__FK_forecasts")
FROM JSON (wrap on) "root" PK "__KEY_root";

[metadata]:
LOAD	[language],
	[transaction_id],
	[version],
	[location_id],
	[units],
	[expire_time_gmt],
	[status_code],
	[__FK_metadata] AS [__KEY_root]
RESIDENT RestConnectorMasterTable
WHERE NOT IsNull([__FK_metadata]);


[forecasts]:
LOAD	[class],
	[expire_time_gmt_u0],
	[fcst_valid],
	[fcst_valid_local],
	[num],
	[day_ind],
	[temp],
	[dewpt],
	[hi],
	[wc],
	[feels_like],
	[icon_extd],
	[wxman],
	[icon_code],
	[dow],
	[phrase_12char],
	[phrase_22char],
	[phrase_32char],
	[subphrase_pt1],
	[subphrase_pt2],
	[subphrase_pt3],
	[pop],
	[precip_type],
	[qpf],
	[snow_qpf],
	[rh],
	[wspd],
	[wdir],
	[wdir_cardinal],
	[gust],
	[clds],
	[vis],
	[mslp],
	[uv_index_raw],
	[uv_index],
	[uv_warning],
	[uv_desc],
	[golf_index],
	[golf_category],
	[severity],
	[__FK_forecasts] AS [__KEY_root]
RESIDENT RestConnectorMasterTable
WHERE NOT IsNull([__FK_forecasts]);


[root]:
LOAD	[__KEY_root]
RESIDENT RestConnectorMasterTable
WHERE NOT IsNull([__KEY_root]);


DROP TABLE RestConnectorMasterTable;




